{
  "name": "maw-files",
  "version": "1.2.1",
  "scripts": {
    "ng": "ng",
    "report-unused-vars": "eslint --rule 'no-unused-vars: 0' --rule '@typescript-eslint/no-unused-vars: 2' . --ext .ts",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve --public-host filesdev.mikeandwan.us",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev_watch": "ng build --vendor-chunk=false --watch",
    "dev_build": "ng build --vendor-chunk=false && ng lint",
    "containerdev_build": "ng build --aot --configuration containerdev",
    "prod_build": "ng build --aot --prod",
    "containerize_dev": "buildah bud -f Containerfile.dev -t maw-files-dev",
    "analyze": "source-map-explorer dist/main*.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.5",
    "@angular/cdk": "^11.2.4",
    "@angular/common": "^11.2.5",
    "@angular/compiler": "^11.2.5",
    "@angular/core": "^11.2.5",
    "@angular/forms": "^11.2.5",
    "@angular/material": "^11.2.4",
    "@angular/platform-browser": "^11.2.5",
    "@angular/platform-browser-dynamic": "^11.2.5",
    "@angular/router": "^11.2.5",
    "@microsoft/signalr": "^5.0.2",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/entity": "^10.1.2",
    "@ngrx/router-store": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "angular-oauth2-oidc": "^10.0.3",
    "date-fns": "^2.16.1",
    "file-saver": "^2.0.5",
    "ng2-file-upload": "^1.4.0",
    "pretty-bytes": "^5.5.0",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.4",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "^1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@angular/cli": "^11.2.4",
    "@angular/compiler-cli": "^11.2.5",
    "@angular/language-service": "^11.2.5",
    "@ngrx/schematics": "^10.1.2",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.19.6",
    "@types/pretty-bytes": "~5.2.0",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-ngrx": "^1.13.0",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.5"
  }
}
